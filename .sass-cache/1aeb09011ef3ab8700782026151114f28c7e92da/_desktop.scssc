:@template"£$/* Desktop and tablet */
@media all and (min-width: 992px) {
  body {
    background: url(../images/bg-page.png) repeat;
  }

  .resp-true {
    display: none;
  }
  
  h2 {
  }
  
  a.button {
    display: inline-block;
    width: auto;
    padding-left: 3em;
    padding-right: 3em;
    margin-bottom: 10px;
  }
  
  /* Narrow pages */
  body.narrow-page {
    .container {
      width: 740px;
    }
    h1.title {
      margin-left: 0;
    }
  }

  #header {
	  .logo {
	    a {
	      width: 67px;
	      height: 70px;
	      background-position: left -143px;
	      float: left;
	      margin-right: 20px;
	    }
	    h1, h2 {
	      display: block;
	      font-family: Arial;
	    }
	    h1 {
	      margin: 0;
	      font-weight: bold;
	      font-size: 40px;
	    }
	    h2 {
	      margin: 0;
	      font-weight: lighter;
	      font-size: 17px;
	    }
	  }
  }
  
  #main {
	  .container {
	    background: $white;
	    margin-top: 30px;
	  }
	  
	  .main-content {
	    padding-bottom: 3em;
	    margin-bottom: 1em;
	    .inner {
	      background: #f4f4f4 url(../images/bg-main.png) no-repeat left top;
	      padding: 20px;
	    }
	    &.dashboard {
	      .inner {
	        padding: 0;
	      }
	    }
	  }
  }
  
  h1 {
    &.title {
      &.full-width {
        margin-left: 0;
      }
    }
  }
  
  /* "Splash page" */
  .left {
    width: 50%;
    float: left;
  }
  
  .right {
    width: 50%;
    float: left;
  }
  
  /* Main menu */
  #main {
    .sidebar-first {
      ul {
        font-size: 0.95em;
        margin: 0;
        padding: 0;
        li {
          list-style: none;
          color: $lightGrey;
          line-height: 36px;
          border-bottom: 1px dotted $lightGrey;          
          a {
            padding-left: 5px;
            color: $darkGrey;
            display: block;
            height: 36px;
            overflow: hidden;
	          &.title {
	            text-transform: uppercase;
	          }
	          &:hover {
	            background: $menuGreyText;
	            text-decoration: none;
	          }
	          &.active {
	            background: url(../images/sprite.png) no-repeat right -256px;
	            color: white;
	            position: relative;
	            z-index: 100;
	            width: 120%;
	          }
          }
          &.last {
            border-bottom: none;
          }
          ul {
            li {
              border-bottom: none;
              a {
                padding-left: 20px;
              }
              ul {
                li {
                  a {
                    padding-left: 40px;
                  }
                  border-bottom: none;
                }
              }
            }
          }
        }
      }
    }
  }  

  h1.title {
    margin-left: 25%;
    padding-left: 10px;
  }
    
  /* Registration Steps */
  ul.steps {
    li.step {
      a {
	      background: url(../images/sprite.png) no-repeat left -55px;    
	      display: block;
	      height: 28px;
	    }
	    span {
	      display: none;
	    }
	    &.step-0 {
	      width: 63px;
	      background: url(../images/sprite.png) no-repeat left -55px;
	    }
	    &.step-1 {
	      width: 52px;
	    }
	    &.step-2 {
	      width: 55px;
	    }
      &.step-3 {
	      width: 49px;
	    }

	  }
	  &.steps-0 {
      li.step-1 {
        a {
          background-position: -63px -55px;
        }
      }
      li.step-2 {
        a {
          background-position: -115px -55px;
        }
      }
      li.step-3 {
        a {
          background-position: -170px -55px;
        }
      }
	  }
	  &.steps-1 {
	    li.step-0 {
	      background-position: left -83px;
	    }
	    li.step-1 {
	      a {
	        background-position: -63px -83px;
	      }
	    }
	    li.step-2 {
	      a {
	        background-position: -115px -83px;
	      }
	    }
	    li.step-3 {
	      a {
	        background-position: -170px -83px;
	      }
	    }
	  } 
	  &.steps-2 {
	    li.step-0 {
	      background-position: left -112px;
	    }
	    li.step-1 {
	      a {
	        background-position: -63px -112px;
	      }
	    }
	    li.step-2 {
	      a {
	        background-position: -115px -112px;
	      }
	    }
	    li.step-3 {
	      a {
	        background-position: -170px -112px;
	      }
	    }
	  } 
	  &.steps-3 {
	    li.step-0 {
	      background-position: left -472px;
	    }
	    li.step-1 {
	      a {
	        background-position: -63px -472px;
	      }
	    }
	    li.step-2 {
	      a {
	        background-position: -115px -472px;
	      }
	    }
	    li.step-3 {
	      a {
	        background-position: -170px -472px;
	      }
	    }
	  } 
  }

  

  /* Forms */
  .form-item {
    .form-text, .form-email, .form-password, select {
      width: 90%;
    }
    textarea {
      width: 90% !important;
    }
    span.validation {
      float: right;
      width: 30px;
      height: 30px;
      &.false {
        background: url(../images/sprite.png) no-repeat right -297px;
      }
      &.true {
        background: url(../images/sprite.png) no-repeat right -327px;
      }
    }
    .message {
      font-size: 0.9375em;
      &.error {
        color: $red;        
      }
    }
  }  
  
  /* General Preloader */
  #preloader-general {
    max-width: 1140px;
    margin-left: auto;
    margin-right: auto;
  }
  
  /* Items */
  ul.items {
    li.item {
      .actions {
        a {
          display: inline-block;
          padding-left: 20px;
          padding-right: 20px;
          font-size: 1em !important;
        }
        span.late {
          font-size: 1em;
        }
      }
    }
  }

  .section {
    .item {
      .value {
        font-weight: normal !important;
      }
    }
  }
  
  .footer {
    margin-top: 10px;
    text-align: right;
    color: $lightGrey;
    font-size: 0.875em;
    a {
      color: $lightGrey;
    }
  }
  
  .no-bg {
    background: none !important;
    border: none !important;
    color: $darkGrey !important;
    a {
      color: $darkGrey !important;
      border-bottom: 1px solid $darkGrey !important;
    }
  }

  .form-submit {
	  @include rounded;
	  &.next {
	    padding-right: 40px !important;
	    &.red {
	      background-position: right -411px !important;
	    }
	    &.blue {
	      background-position: right -411px !important;
	      &:active, &:focus {
	        background-color: $darkBlue;
	      }
	    }
	  }
  }
  
  form.narrow {
    width: 60%;
  }
  
  /* Profile page */
  .section {
	  .item {
	    clear: both;
			.label {
        width: 20%;
        float: left;
        text-align: left;
			}
			.value {
			  float: left;
	    }
	  }
  }

  /* Lost Password */
  a.lost-password {
    float: right;
    margin-right: -130px;
    margin-top: 5px;
  }
  
  .lost-password {
    &.form-submit {
      width: 105px;
    }
  }
  
  /* Consistent buttons */
  .form-submit {
    &.equal-width {
      width: 120px;
    }
  }
  
  /* Dashboard */
  .dashboard {
    .section {
      border-bottom: 15px solid white;
      padding: 20px; 
      h3 {
        margin-top: 0;
      }
      .actions {
        .col {
          padding-right: 20px !important;
        }
        a.button {
          width: 100%;
          padding: 6px 10px !important;
        }
      }
    }
  }
  
  /* Extra space right */
  .space-left {
    margin-left: 20px;
  }
  .space-right {
    margin-right: 20px;
  }
  
  /* Bibliotheek pagina */
  .library {
    .inner {
      padding: 0 !important;
      background: white !important;
    }
    section {
      &.first {
	      background: $bgMainContent url(../images/bg-title.png) no-repeat left top;
        margin-bottom: 0 !important;
      }
    }
    section.section, section.links {
      background: #f4f4f4;
      margin-bottom: 15px;
      padding: 20px;
      padding-bottom: 0;
    }
    section.betaling, section.historiek {
      padding-bottom: 20px;
    }
    section.messages {
      padding: 20px;
    }
    section.icons {
      background: white;
    }
    
    
    ul.items {
      li.item {
        .actions {
          text-align: center;
        }
      }
    }
    
    table.uitleenhistoriek {
      width: 100%;
      margin-bottom: 20px;
      tr {
        &.odd {
          background: $lightBlue;
        }
        td, th {
          padding: 10px 4px;
          &.title {
            width: 40%;
          }
          &.dates, &.library {
            width: 30%;
          }
        }
      }
    }

    table {
      width: 100%;
      margin-bottom: 20px;
      tr {
        &.odd {
          background: $lightBlue;
        }
        td, th {
          padding: 10px 4px;
        }
      }
    }

    table.kosten {
      tr {
        td, th {
          &.title {
            width: 60%;
          }
          &.type, &.date, &.bedrag {
            width: 12%;
          }
        }
      }
    }

    table.uitleenhistoriek {
      tr {
        td, th {
          &.title {
            width: 40%;
          }
          &.dates, &.library {
            width: 30%;
          }
        }
      }
    }

    
    a.download {
      display: block;
      margin-left: 70%;
      width: 30%;
    }
    
  }
  
}

/* Desktop only */
@media all and (min-width: 1023px) {
  #wrapper {
    margin-left: auto;
    margin-right: auto;
  }
  
  .button, .form-submit {
    font-size: 1em;
    font-weight: 300;
    padding: 6px 14px !important;
  }
